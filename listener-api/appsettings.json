{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "AllowedHosts": "*",
  "Spring": {
    "Application": {
      "Name": "ListenerService"
    },
    "Cloud": {
      "Config": {
        "Uri": "http://configserver:8888",
        "ValidateCertificates": false,
        "fail_fast": true,
        "Env": "Production"
      }
    }
  },
  "Eureka": {
    "Client": {
      "ShouldRegisterWithEureka": true,
      "ServiceUrl": "http://eurekaserver:8761/eureka/",
      "ValidateCertificates": false
    },
    "Instance": {
      "AppName": "ListenerService",
      "HostName": "listener-api",
      "Port": "5100"
    }
  }
}
